{
  "publisher": "julesmons",
  "name": "recline",
  "displayName": "Recline",
  "version": "0.2.5",
  "packageManager": "pnpm@9.15.2",
  "description": "Recline: The autonomous AI assistant that seamlessly integrates with your CLI and editor to create, edit, and run; redefining how you code.",
  "author": {
    "name": "Jules Mons",
    "email": "jules@jupdev.nl",
    "url": "https://www.jupdev.nl/"
  },
  "contributors": [
    {
      "name": "Cline Bot Inc.",
      "url": "https://cline.bot/"
    },
    {
      "name": "Saoud Rizwan",
      "url": "https://github.com/saoudrizwan/"
    }
  ],
  "license": "MPL-2.0",
  "funding": "https://github.com/sponsors/julesmons",
  "homepage": "https://github.com/julesmons/recline#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julesmons/recline.git"
  },
  "bugs": "https://github.com/julesmons/recline/issues",
  "keywords": [
    "AI",
    "recline",
    "cline",
    "in-IDE",
    "developer tools",
    "dev",
    "mcp",
    "openrouter",
    "coding",
    "agent",
    "autonomous",
    "chatgpt",
    "sonnet",
    "ai",
    "llama"
  ],
  "categories": [
    "Other"
  ],
  "main": "dist/extension.js",
  "icon": "assets/icons/recline.png",
  "galleryBanner": {
    "color": "#3B82F6",
    "theme": "dark"
  },
  "files": [
    "CLINE.LICENSE.md",
    "LICENSE.md",
    "assets",
    "dist"
  ],
  "engines": {
    "vscode": "^1.93.0"
  },
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "recline-ActivityBar",
          "title": "Recline",
          "icon": "assets/icons/recline.svg"
        }
      ]
    },
    "views": {
      "recline-ActivityBar": [
        {
          "type": "webview",
          "id": "recline.SidebarProvider",
          "name": ""
        }
      ]
    },
    "commands": [
      {
        "command": "recline.plusButtonClicked",
        "title": "New Task",
        "icon": "$(add)"
      },
      {
        "command": "recline.mcpButtonClicked",
        "title": "MCP Servers",
        "icon": "$(server)"
      },
      {
        "command": "recline.historyButtonClicked",
        "title": "History",
        "icon": "$(history)"
      },
      {
        "command": "recline.popoutButtonClicked",
        "title": "Open in Editor",
        "icon": "$(link-external)"
      },
      {
        "command": "recline.settingsButtonClicked",
        "title": "Settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "recline.openInNewTab",
        "title": "Open In New Tab",
        "category": "Recline"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "recline.plusButtonClicked",
          "group": "navigation@1",
          "when": "view == recline.SidebarProvider"
        },
        {
          "command": "recline.mcpButtonClicked",
          "group": "navigation@2",
          "when": "view == recline.SidebarProvider"
        },
        {
          "command": "recline.historyButtonClicked",
          "group": "navigation@3",
          "when": "view == recline.SidebarProvider"
        },
        {
          "command": "recline.popoutButtonClicked",
          "group": "navigation@4",
          "when": "view == recline.SidebarProvider"
        },
        {
          "command": "recline.settingsButtonClicked",
          "group": "navigation@5",
          "when": "view == recline.SidebarProvider"
        }
      ]
    }
  },
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild build --watch",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "package": "vsce package --no-dependencies",
    "vscode:prepublish": "pnpm build",
    "test": "pnpm typecheck && vitest && vscode-test",
    "test:unit": "vitest",
    "test:e2e": "vscode-test",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.12.0",
    "@anthropic-ai/sdk": "^0.33.1",
    "@anthropic-ai/vertex-sdk": "^0.6.1",
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.1.0",
    "@reexport/fd-prebuilt": "^1.2.1",
    "@reexport/fd-prebuilt-darwin-arm64": "^1.2.1",
    "@reexport/fd-prebuilt-darwin-x64": "^1.2.1",
    "@reexport/fd-prebuilt-linux-x64-gnu": "^1.2.1",
    "@reexport/fd-prebuilt-linux-x64-musl": "^1.2.1",
    "@reexport/fd-prebuilt-win32-x64-msvc": "^1.2.1",
    "@vscode/codicons": "^0.0.36",
    "@vscode/ripgrep": "^1.15.9",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "axios": "^1.7.9",
    "bufferutil": "^4.0.9",
    "debounce": "^2.2.0",
    "default-shell": "^2.2.0",
    "delay": "^6.0.0",
    "diff": "^7.0.0",
    "es-toolkit": "^1.31.0",
    "fuse.js": "^7.0.0",
    "isbinaryfile": "^5.0.4",
    "mammoth": "^1.9.0",
    "monaco-vscode-textmate-theme-converter": "^0.1.7",
    "openai": "^4.77.3",
    "os-name": "^6.0.0",
    "p-wait-for": "^5.0.2",
    "pdf-parse": "^1.1.1",
    "preact": "^10.25.4",
    "puppeteer-chromium-resolver": "^23.0.0",
    "puppeteer-core": "^23.11.1",
    "react-remark": "^2.1.0",
    "react-textarea-autosize": "^8.5.6",
    "react-use": "^17.6.0",
    "react-virtuoso": "^4.12.3",
    "rehype-highlight": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.0",
    "remark-stringify": "^11.0.0",
    "sanitize-html": "^2.14.0",
    "serialize-error": "^11.0.3",
    "strip-ansi": "^7.1.0",
    "styled-components": "^6.1.14",
    "tree-sitter-wasms": "^0.1.12",
    "undici": "^7.2.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "utf-8-validate": "^6.0.5",
    "web-tree-sitter": "^0.24.6",
    "web-vitals": "^4.2.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@rsbuild/core": "^1.1.13",
    "@rsbuild/plugin-preact": "^1.2.0",
    "@types/diff": "^6.0.0",
    "@types/node": "^22.10.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/sanitize-html": "^2.13.0",
    "@types/vscode": "^1.93.0",
    "@types/vscode-webview": "^1.57.5",
    "@vscode/vsce": "^3.2.1",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "lint-staged": "^15.3.0",
    "repomix": "^0.2.16",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
